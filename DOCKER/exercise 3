#--------------------------------docker-compose.yaml---------------------------------USING  .YAML----------------------------------------
version: '3'
services:
 nginx:
  image: nginx:1.19.10
  ports:
   - 8080:80
 #-------------------------------------------OR--------------------------------------USING  DOCKER COMMAND-----------------------------------------------------
 
 docker run -p 8080:80 --name nginx nginx:1.19.10 
